    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch-open",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "open",
                "main.go",
            ]
        },
        {
            "name": "Launch-preview",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "preview",
                "localhost:24000",
            ]
        }
    ]
}